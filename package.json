{
  "name": "airbnb-property-manager",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "description": "Production-grade Airbnb property management app with cleaning, maintenance, and team coordination",
  "keywords": [
    "expo",
    "react-native",
    "property-management",
    "airbnb",
    "cleaning",
    "maintenance"
  ],
  "author": "Airbnb Property Management Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/airbnb-property-manager.git"
  },
  "scripts": {
    "start": "expo start",
    "dev": "expo start --clear",
    "dev:clean": "npx expo start --clear --reset-cache",
    "dev:tunnel": "expo start --tunnel",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:release": "eas build --platform android --profile production",
    "ios": "expo run:ios",
    "ios:release": "eas build --platform ios --profile production",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.{ts,tsx}'",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:expo": "expo lint",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test",
    "build": "expo export --platform all",
    "build:web": "expo export --platform web --clear",
    "prebuild": "expo prebuild --clean",
    "doctor": "expo-doctor --verbose",
    "docs:precheck": "node scripts/precheck-docs.js",
    "docs:update": "npm run docs:precheck && node scripts/update-manifests.js",
    "docs:validate": "npm run docs:precheck && node scripts/validate-docs.js",
    "verify-config": "node verify-android-config.js",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "update:check": "npx npm-check-updates",
    "update:interactive": "npx npm-check-updates -i",
    "analyze:bundle": "npx react-native-bundle-visualizer",
    "clean": "watchman watch-del-all && rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && npm install",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "fix:dependencies": "node scripts/fix-dependencies.js",
    "fix:dependencies:win": "scripts/fix-dependencies.bat",
    "fix:dependencies:unix": "node scripts/fix-dependencies.js"
  },
  "dependencies": {
    "@expo/metro-config": "~0.20.14",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@gluestack-ui/nativewind-utils": "^1.0.24",
    "@gluestack-ui/themed": "^1.1.63",
    "@gorhom/bottom-sheet": "^5.2.2",
    "@hookform/resolvers": "^3.9.1",
    "@legendapp/motion": "^2.4.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/virtualized-lists": "^0.82.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@sentry/react-native": "^6.5.0",
    "@shopify/flash-list": "^1.7.3",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-persist-client": "^5.64.2",
    "aria-hidden": "^1.2.6",
    "date-fns": "^4.1.0",
    "expo": "54.0.13",
    "expo-application": "~6.0.7",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "~18.0.8",
    "expo-crypto": "~15.0.5",
    "expo-device": "~8.0.7",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "~15.0.7",
    "expo-localization": "~18.0.7",
    "expo-notifications": "~0.31.1",
    "expo-router": "~6.0.12",
    "expo-secure-store": "~14.0.5",
    "expo-splash-screen": "~31.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~0.27.11",
    "expo-web-browser": "~15.0.8",
    "i18n-js": "^4.4.3",
    "lottie-react-native": "^7.4.0",
    "moti": "^0.29.0",
    "nativewind": "^4.1.23",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.54.2",
    "react-native": "0.81.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^3.1.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "~15.11.2",
    "react-native-svg-transformer": "~1.5.1",
    "react-native-toast-message": "^2.2.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "~0.5.1",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.3.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@expo/cli": "~0.24.14",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@nx/eslint-plugin": "^22.0.1",
    "@nx/expo": "22.0.1",
    "@nx/jest": "22.0.1",
    "@nx/js": "22.0.1",
    "@nx/react": "22.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tanstack/eslint-plugin-query": "^5.64.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/node": "20.19.9",
    "@types/react": "~19.2.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-preset-expo": "~13.2.0",
    "detox": "^20.29.1",
    "eslint": "^8.56.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-expo": "^54.0.12",
    "jest-util": "^30.0.2",
    "lint-staged": "^15.3.0",
    "metro-config": "~0.82.4",
    "metro-resolver": "~0.82.4",
    "nx": "22.0.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "react-test-renderer": "^19.2.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.tsx"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "device": {
          "type": "iPhone 15 Pro"
        },
        "app": "ios.debug"
      },
      "android.emu.debug": {
        "device": {
          "avdName": "Pixel_5_API_33"
        },
        "app": "android.debug"
      }
    }
  },
  "nx": {}
}
