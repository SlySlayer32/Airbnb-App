{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(react-native/(.*)$)|^(react-native$)",
    "^(expo(.*)$)|^(@expo(.*)$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/types(.*)$",
    "^@/utils(.*)$",
    "^@/services(.*)$",
    "^@/contexts(.*)$",
    "^@/components(.*)$",
    "^@/constants(.*)$",
    "^@/data(.*)$",
    "^@/app(.*)$",
    "",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
