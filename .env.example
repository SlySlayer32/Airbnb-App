# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
EXPO_PUBLIC_APP_NAME=Airbnb Property Manager
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_ENVIRONMENT=development

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.airbnb-property-manager.com
EXPO_PUBLIC_API_TIMEOUT=30000

# Sentry Configuration (Optional)
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# Analytics Configuration (Optional)
EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false
EXPO_PUBLIC_ENABLE_PERFORMANCE_MONITORING=false

# Development Configuration
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Production Configuration (uncomment for production)
# EXPO_PUBLIC_ENVIRONMENT=production
# EXPO_PUBLIC_DEBUG_MODE=false
# EXPO_PUBLIC_LOG_LEVEL=error
# EXPO_PUBLIC_ENABLE_ANALYTICS=true
# EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
# EXPO_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true
